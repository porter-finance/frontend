{
  "name": "porter-finance",
  "description": "Porter Finance",
  "homepage": "https://porter-app.vercel.app/",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "typechain:generate": "yarn typechain --target=ethers-v5 ./src/constants/abis/**/*.json --out-dir=./gen/types",
    "start": "react-app-rewired start",
    "build": "cross-env REACT_APP_GIT_COMMIT_HASH=$(git show -s --format=%H) yarn typechain:generate && react-app-rewired build",
    "ipfs-build": "cross-env PUBLIC_URL=\".\" react-app-rewired build",
    "ipfs-publish": "ipfs-deploy -p infura -O dist ./build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:fix": "eslint  --fix",
    "prettier:fix": "prettier --write",
    "cy:run": "cypress run",
    "serve:build": "serve -s build -l 3000",
    "integration-test": "yarn build && start-server-and-test 'yarn run serve:build' http://localhost:3000 cy:run",
    "prepare": "husky install",
    "postinstall": "yarn typechain:generate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "GPL-3.0-or-later",
